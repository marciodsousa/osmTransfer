{
    "manifest_version": 2,

    "name": "Transfer List Hack",
    "description": "Simplest chrome extension to run javascript on the transfer list.",
    "version": "1.0",

    "browser_action": {
        "default_title": "Hack my transfer list!",
        "default_icon": "icons/manager16.png"
    },

    "icons": {
        "16": "icons/manager16.png",
        "48": "icons/manager48.png",
        "128": "icons/manager128.png"
    },

    "permissions": ["activeTab"],

    "background": {
        "scripts": ["js/background.js"],
        "persistent": false
    }
}